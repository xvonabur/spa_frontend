{
  "name": "SPA",
  "description": "Frontend application",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build:langs && node server.js",
    "debug": "npm run build:langs && node $NODE_DEBUG_OPTION server.js",
    "build": "webpack --progress --config webpack.production.config.js && npm run build:langs && cp -r build/lang public/dist/lang",
    "build:langs": "babel-node src/util/translator.js",
    "deploy": "aws s3 cp public s3://spa-front --recursive && aws s3 cp .env.production s3://spa-front",
    "test": "jest",
    "test:feature": "forever start server.js && codeceptjs run ; forever stopall"
  },
  "devDependencies": {
    "babel-cli": "^6.22.1",
